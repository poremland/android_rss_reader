		android:layout_height="match_parent">

		<ListView android:id="@android:id/list"
			style="@style/ListStyle"
			android:layout_width="match_parent"
			android:layout_height="match_parent" />
	</FrameLayout>

	<include layout="@layout/progress" />

</FrameLayout>
